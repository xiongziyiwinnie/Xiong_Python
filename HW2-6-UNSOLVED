def translation(s):
    old=s
    new=list(s)
    consonant=['b','c','d','f','g','h','j','k','l','m','n','p','q','r','s','t','v','w','x','z']
    l=len(new)
    i=0
    while i<l:
       for c in consonant:
         if new[i]==c:
           new=new.insert(i,'o'+c)
           i=i+3
           l=l+2
         else: i=i+1
    return ''.joint(new)
